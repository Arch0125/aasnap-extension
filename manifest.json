{
    "manifest_version": 2,
    "name": "Ethereum RPC Interceptor",
    "description": "This extension intercepts Ethereum RPC calls.",
    "version": "1.0",
    "permissions": [
      "<all_urls>"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ]
  }
  